<%= form_for @owner do |f| %>
    <p>
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :email %><br>
        <%= f.text_field :email %>
    </p>
    <p>
        <%= f.label :street_address %><br>
        <%= f.text_field :street_address %>
    </p>
    <p>
        <%= f.label :city %></br>
        <%= f.text_field :city %>
    </p>
    <p>
        <%= f.label :state %><br>
        <%= f.text_field :state %>
    </p>
    <p>
        <%= f.label :zip_code %><br>
        <%= f.text_field :zip_code %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>